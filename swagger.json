  "info": {
    "title": "Wallport API",
    "description": "API for Personal Finance Tracker Application",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "test@example.com"
          },
          "password": {
            "type": "string",
            "example": "password123"
          },
          "full_name": {
            "type": "string",
            "example": "John Doe"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "test@example.com"
          },
          "password": {
            "type": "string",
            "example": "password123"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "ForgotPasswordDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "test@example.com"
          }
        },
        "required": [
          "email"
        ]
      },
      "ResetPasswordDto": {
        "type": "object",
        "properties": {
          "password": {
            "type": "string",
            "example": "newPassword123"
          }
        },
        "required": [
          "password"
        ]
      },
      "CreateWalletDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "My Bank Account"
          },
          "currency": {
            "type": "string",
            "enum": [
              "IDR",
              "USD"
            ],
            "example": "USD"
          },
          "initial_balance": {
            "type": "number",
            "example": 1000
          }
        },
        "required": [
          "name",
          "currency",
          "initial_balance"
        ]
      },
      "UpdateWalletDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "My Primary Bank Account"
          },
          "currency": {
            "type": "string",
            "enum": [
              "IDR",
              "USD"
            ],
            "example": "IDR"
          },
          "initial_balance": {
            "type": "number",
            "example": 1500
          }
        }
      },
      "CreateCategoryDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Salary"
          },
          "type": {
            "type": "string",
            "enum": [
              "income",
              "expense"
            ],
